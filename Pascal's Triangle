class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        row=[[1]*i for i in range(1,numRows+1)]
        print (row)
        i=1
        while i<numRows:
            for j in range(1, len(row[i-1])):
                row[i][j]=row[i-1][j]+row[i-1][j-1]
            i+=1
        return row
